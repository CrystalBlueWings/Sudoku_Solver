{
  "name": "sudoku_solver_js",
  "version": "1.1.0",
  "description": "A Sudoku solver library created for educational purposes.",
  "type": "module",
  "main": "src/",
  "engines": {
    "node": ">=20.6.0"
  },
  "scripts": {
    "start": "node src/test-app.js",
    "test": "jest --detectOpenHandles --verbose --coverage",
    "test:single": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "lint": "npx eslint ./src || exit 0",
    "lint:fix": "npx eslint ./src --fix || exit 0"
  },
  "author": "Lilia Zeleskov <lz222ea@student.lnu.se>",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.25.9",
    "@lnu/eslint-config": "^1.1.10",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "keywords": [
    "sudoku",
    "sudoku-solver",
    "puzzle",
    "solver",
    "game",
    "grid",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CrystalBlueWings/Sudoku_Solver"
  }
}
